<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.websocket.health.mapper.MyBenefitMapper">
    <select id="findAll" resultType="com.websocket.health.pojo.Benefit">
        select * from health.benefit
    </select>
    <select id="queryBenefitByFood" resultType="com.websocket.health.pojo.Benefit">
        select * from health.benefit where id in (select benefit_id from health.food_benefit where food_id=#{arg0})
    </select>
</mapper>