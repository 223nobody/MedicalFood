<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.websocket.health.mapper.MyUserMapper">
    <select id="MaxId" resultType="java.lang.Integer">
        select max(id) from health.user
    </select>
    <select id="queryUser" resultType="com.websocket.health.pojo.User">
        select id,name,account from health.user where account=#{param1}
        <if test="param2!=null">
            and password=#{param2}
        </if>
    </select>
</mapper>